    "server:express": "node --experimental-strip-types server-express.ts",
    "postgraphile": "NODE_OPTIONS=\"--experimental-strip-types\" postgraphile"
  },
  "dependencies": {
    "@graphile/pg-aggregates": "0.2.0-beta.8",
    "postgraphile": "5.0.0-beta.49",
